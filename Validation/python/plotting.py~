import ROOT

filenames = ("aodsim.root", "miniaodsim.root")
canvases = []
histos = []

for filename in filenames:
    h = dict()
    f = ROOT.TFile(filename)
    hNames = f.GetListOfKeys()
    for n in hNames:
        h[n.GetName()] = f.Get(n.GetName())
    histos.append(h)

for k in histos[0]:
    canvases.append(ROOT.TCanvas("c" + k, "c"))
    histos[0][k].Draw()
    histos[1][k].Draw("SAME")
                 
